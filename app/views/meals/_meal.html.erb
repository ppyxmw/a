<%- model_class = Meal -%>
<% @meals.each do |meal| %>
  <div class="card">
    <section class="picture">
      <%= image_tag(meal.image.url, size: "300x300") if meal.image %>
    </section>
    <section class="post-links">
      <a class="postid"><%= meal.chef.email %></a>
      <a class="submitter">4.5 stars</a>
    </section>
	  <section class="description">
      <%= meal.description %>
    </section>
  </div>
<% end %>
